{
  "name": "user-status",
  "displayName": "user-status",
  "description": "Allows developers to change their GitHub and Slack status easily",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:user-status.helloWorld",
    "onCommand:user-status.mlh",
    "onCommand:user-status.showup"

  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "user-status.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "user-status.mlh",
        "title": "Major League Hacking"
      },
      {
        "command": "user-status.showup",
        "title": "Alert"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
